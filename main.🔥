from max.driver import Tensor, cup_device 

struct MyPair:
    var first: Int 
    var second: Int

    fn __init__(inout self, first: Int, second: Int):
        self.first = first
        self.second = second
    fn dump(self):
        print(self.first)
        print(self.second)
trait SomeTrait:
    fn required_method(self, x: Int): ...

@value
struct SomeStruct(SomeTrait):
    fn required_method(self, x: Int):
        print('---> ', x)

fn func_with_trait[T: SomeTrait](x: T):

    x.required_method(3)

fn use_trait_func():
    var thing = SomeStruct()
    func_with_trait(thing)


fn main():
    var x = MyPair(1,2)
    x.dump()
    use_trait_func()
