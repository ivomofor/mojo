from max.graph import TensorType, Type, Graph

fn main() raises:

    # initialize the graph
    var graph = Graph(in_types = List[Type](TensorType(DType.float32, 1), TensorType(DType.float32, 2))) 

    # create a symbolic representation of addition 
    var out = graph[0] + graph[1]

    # designate out as the output of the graph 
    graph.output(out)

    # print out the graph's output 
    print(graph)